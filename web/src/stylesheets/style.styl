.dark-primary-color
  background #689F38

.default-primary-color
  background #8BC34A

.default-primary-text-color
  color #8BC34A

.light-primary-color
  background #DCEDC8

.text-primary-color
  color #212121

.accent-color
  background-color #03A9F4 !important

.primary-text-color
  color #212121

.secondary-text-color
  color #727272

.divider-color
  border-color #B6B6B6

.white-text
  color #FFF

.btn:hover
  background #689F38

.waves-effect.waves-accent .waves-ripple
  background #03A9F4

.card-reveal-layout
  display flex
  flex-direction column
  align-items stretch

.card-reveal-item
  display flex
  flex 1
  align-items stretch
  flex-direction column

.card-reveal-footer
  display flex
  flex: 1
  align-items flex-end
  justify-content flex-end

.flex-row
  display flex
  flex-direction row
  align-items center
  justify-content flex-start

