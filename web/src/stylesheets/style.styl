nav ul a
  font-weight 600

p
  line-height 2rem

.button-collapse
  color #26a69a

.staggered-wrapper
  position relative

.staggered-wrapper ul
  width 80px
  position absolute
  right 0px

.staggered-wrapper li
  opacity 0

.parallax-container
  min-height 380px
  line-height 0
  height auto
  color rgba(255, 255, 255, .9)

.parallax-container .section
  width 100%

.banner
  display flex
  height 60vh

footer.page-footer
  margin 0

